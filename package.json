{
	"name": "@tatamo/parsergenerator",
	"version": "0.1.0-2",
	"description": "simple lr(1) parser generator written in TypeScript",
	"keywords": ["parser", "generator", "lexer", "parsing", "compiler"],
	"main": "dist/index.js",
	"types": "dist/index.d.ts",
	"scripts": {
		"test": "jest",
		"test-w": "jest --watchAll",
		"build": "gulp",
		"watch": "gulp watch"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/Tatamo/parsergenerator"
	},
	"author": "Tatamo",
	"license": "BSD-3-Clause",
	"devDependencies": {
		"@types/jest": "^20.0.5",
		"@types/node": "^8.0.14",
		"gulp": "^3.9.1",
		"gulp-sourcemaps": "^2.6.0",
		"gulp-tslint": "^8.1.1",
		"gulp-typescript": "^3.2.1",
		"jest": "^20.0.4",
		"merge2": "^1.1.0",
		"ts-jest": "^20.0.7",
		"tslint": "^5.5.0",
		"typescript": "^2.1.1"
	},
	"dependencies": {},
	"jest": {
		"transform": {
			"^.+\\.tsx?$": "<rootDir>/node_modules/ts-jest/preprocessor.js"
		},
		"testRegex": "(/__tests__/(?!data/).*|\\.(test|spec))\\.(tsx?|jsx?)$",
		"moduleFileExtensions": [
			"ts",
			"js",
			"json"
		],
		"globals": {
			"ts-jest": {
				"tsConfigFile": "__tests__/tsconfig.json"
			}
		}
	}
}
